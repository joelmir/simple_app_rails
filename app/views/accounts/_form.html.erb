<%= simple_form_for(@account) do |f| %>
  <%= f.input :name %>
  <%= f.input :expiration_date , as: :string,  input_html: { class: 'datepicker', value: @account.expiration_date.nil? ? "" : @account.expiration_date.strftime("%d/%m/%Y") } %>
  <% @users.each do |user| %>
    <%= check_box_tag :user_ids, user.id, @account.users.include?(user), :name => 'account[user_ids][]' %>
    <%= user.name %><br />
  <% end %>
  <% @juridical_people.each do |juridical_person| %>
    <%= check_box_tag :juridical_person_ids, juridical_person.id, @account.juridical_people.include?(juridical_person), :name => 'account[juridical_person_ids][]' %>
    <%= juridical_person.name %><br />
  <% end %>
  <%= f.button :submit %>
<% end %>
